{
"manifest_version": 2,
"name": "Domain Control",
"version": "1.0.36",
"description": "Block unwanted content with a few clicks.",
"icons": {
	"128": "icon128.png",
	"16": "icon16.png",
	"48": "icon48.png"
},
"browser_action": {
	"default_popup": "popup.html",
	"default_title": "Domain Control"
},
"author": {
	"email": "aigeros.innov(a)gmail.com"
},
"background": {
	"scripts": ["list.js", "background.js"]
},
"options_page": "options.html",
"options_ui": {
	"chrome_style": false,
	"page": "options.html"
},
"minimum_chrome_version": "40.0",
"permissions": ["tabs", "http://*/*", "https://*/*", "webRequest", "webRequestBlocking", "unlimitedStorage", "storage", "contextMenus"],
"update_url": "https://clients2.google.com/service/update2/crx"
}
